
<div class="content">
    <div class="content">

        <div class="box" id="first-box">
            <div class="box-header with-border">
                <h3 class="box-title">Forms</h3>
                <a href="/createForms"> <button type="button" class="btn btn-primary pull-right">New Form</button></a>
            </div>
            <div class="box-body">

                <table id="created_forms" class="table">
                    <thead>
                    <tr>
                        <th>ID</th>
                        <th>Form Name</th>
                        <th>Category</th>
                        <th>Created By</th>
                        <th>Created At</th>
                        <th>Publish</th>
                        <th>More Details</th>
                        <th>Edit</th>
                    </tr>
                    </thead>
                    <tfoot>
                    <tr>
                        <th>ID</th>
                        <th>Form Name</th>
                        <th>Category</th>
                        <th>Created By</th>
                        <th>Created At</th>
                        <th>Publish</th>
                        <th>More Details</th>
                        <th>Edit</th>
                    </tr>
                    </tfoot>
                </table>
            </div>

        </div>

        <div class="box hidden" id="filled-table-box">
            <div class="box-header with-border">
                <h3 class="box-title">Filled Forms</h3>
            </div>
            <div class="box-body">
                <div class="form-group">
                    <label for="fromToRange">Date range:</label>
                    <div id="fromToRangeWage" class="form-control input-group"
                         style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc;margin-bottom:5px ;width: 50%">
                        <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>&nbsp;
                        <span></span> <b class="caret"></b>
                    </div>
                </div>
                <div class="form-group">
                    <table id='filled-table' class='table'></table>
                </div>

            </div>
            <div class="box-footer">
                <button type="button" class="btn btn-primary" id="go-back-onDetails">Go Back</button>
                <button type="button" class="btn btn-danger pull-right" id="deleteRow">Delete Data</button>
            </div>
        </div>

        <div class="loader hidden"></div>

        <div class="row hidden" id="edit-form">
            <div class="col-sm-3">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">Options</h3>
                    </div>
                    <div class="box-body">
                        <div class="form-group">
                            <label for="editCategory" class="col-sm-2 control-label">Categories</label>
                            <select required id="editCategory" name="editCategory" class="form-control">

                            </select>
                        </div>
                        <div class="form-group">
                            <label for="editPublished" class="col-sm-2 control-label">Publish</label>
                            <select required id="editPublished" name="editPublished" class="form-control">
                                <option value="Yes">Yes</option>
                                <option value="No">No</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-sm-9">
                <div class="box">
                    <div class="box-header with-border">

                        <h3 class="box-title edit-form-title">Form Name</h3>
                    </div>

                        <div class="box-body">
                            <div class="form-editing">
                                <div class="row" hidden>
                                    <div class="form-group">
                                        <label for="edit-id" class="col-sm-2 control-label">ID</label>

                                            <input type="text" class="form-control" name="edit-id" id="edit-id">

                                    </div>
                                </div> <!--id-->
                                <div class="form-group">
                                    <label for="form-edit-title" id="title-label" class="control-label">Form Name </label><span class="required-asterisk" style="display: inline;"> *</span>
                                    <input type="text" name="form-edit-title" id="form-edit-title" class="form-control">

                                </div>
                                <div class="form-group">
                                    <div class="fb-editor"></div>
                                </div>
                            </div>
                            <div class="edit-fb-render">
                            </div>



                        </div>
                        <div class="box-footer">
                            <button class="btn btn-success formRender-edit-form">Edit</button>
                            <button class="btn btn-default go-back" onclick="window.location.reload()">Go Back</button>
                            <button class="btn btn-danger go-back-without-modification">Go Back</button>

                        </div>

                </div>
            </div>
        </div>


    </div>


</div>


